<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FreeAreaActivity">


    <ImageView
        android:id="@+id/background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/second_page_background_color" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="@dimen/logo_size_land"
        android:layout_height="@dimen/logo_size_land"
        android:src="@drawable/second_logo"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/margin15"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/margin10"/>

    <ImageView
        android:id="@+id/second_page_pic"
        android:layout_width="450dp"
        android:layout_height="wrap_content"
        android:src="@drawable/second_page_map"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/logo"
        android:layout_marginStart="@dimen/margin10"
        android:adjustViewBounds="true"
        android:layout_marginTop="@dimen/margin10"/>

    <Button
        android:id="@+id/btn_google_maps"
        android:layout_width="@dimen/btn_width100"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/where_am_i"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/second_page_pic"
        app:layout_constraintTop_toBottomOf="@+id/back"
        android:layout_marginTop="@dimen/margin50"
        android:textSize="@dimen/btn_width16"
        android:background="@drawable/btn_google_maps_border"
        android:textColor="@color/background_color"/>

    <Button
        android:id="@+id/btn_a"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/a"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="@dimen/margin20"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_b"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/b"
        app:layout_constraintStart_toEndOf="@id/btn_a"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_bb"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/bb"
        app:layout_constraintStart_toEndOf="@id/btn_b"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_bk"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/bk"
        app:layout_constraintStart_toEndOf="@id/btn_bb"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_d"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/d"
        app:layout_constraintStart_toEndOf="@id/btn_bk"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_eh"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/eh"
        app:layout_constraintStart_toEndOf="@id/btn_d"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_f"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/f"
        app:layout_constraintStart_toEndOf="@id/btn_eh"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_g"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/g"
        app:layout_constraintStart_toEndOf="@id/btn_eh"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_k"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/k"
        app:layout_constraintStart_toEndOf="@id/btn_g"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_m"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/m"
        app:layout_constraintStart_toEndOf="@id/btn_k"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <Button
        android:id="@+id/btn_n"
        android:layout_width="@dimen/btn_width60"
        android:layout_height="@dimen/btn_height60"
        android:text="@string/n"
        app:layout_constraintStart_toEndOf="@id/btn_m"
        android:layout_marginStart="@dimen/margin5"
        app:layout_constraintTop_toBottomOf="@id/second_page_pic"
        android:layout_marginTop="@dimen/margin10"
        android:background="@drawable/button_second_page60"
        android:textSize="@dimen/btn_text_size"
        android:textColor="@color/second_page_background_color"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btn_logout"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="@dimen/margin20"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/margin20"
        app:srcCompat="@drawable/exit"
        app:backgroundTint="@color/background_color"
        app:tint="@color/second_page_background_color"/>

    <Button
        android:id="@+id/back"
        android:layout_width="@dimen/btn_width50"
        android:layout_height="@dimen/btn_height30"
        android:background="@drawable/back_four"
        app:layout_constraintTop_toBottomOf="@id/logo"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="@dimen/margin30"/>

</androidx.constraintlayout.widget.ConstraintLayout>